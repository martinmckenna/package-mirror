{
  "name": "packagemirror",
  "description": "mirror jawn",
  "version": "0.1.0",
  "type": "module",
  "repository": {
    "type": "git",
    "url": "https://github.com/martinmckenna/package-mirror.git"
  },
  "private": true,
  "engines": {
    "node": ">=20"
  },
  "sideEffects": false,
  "scripts": {
    "start": "react-router dev",
    "build": "react-router build",
    "serve:production": "react-router-serve build/server/index.js",
    "clean": "find . -name \"node_modules\" -exec rm -rf '{}' +",
    "lint": "eslint 'app' --ignore-pattern '*.test.*' --ignore-pattern '*.spec.*' --ext '.ts,.tsx'",
    "format": "prettier --write \"app/**/*.{ts,tsx}\" \"app/**/*.{js}\" --no-error-on-unmatched-pattern",
    "prepare": "husky install",
    "type-check": "react-router typegen && tsc"
  },
  "dependencies": {
    "@mjackson/form-data-parser": "0.9.1",
    "@pabra/sortby": "1.0.2",
    "@reach/dialog": "0.18.0",
    "@reach/menu-button": "0.18.0",
    "@reach/tabs": "0.18.0",
    "@react-router/node": "7.9.4",
    "@react-router/serve": "7.8.0",
    "axios": "1.10.0",
    "cookie": "0.7.0",
    "copy-to-clipboard": "3.3.3",
    "fast-equals": "4.0.1",
    "feed": "4.2.2",
    "form-data": "4.0.0",
    "image-size": "1.0.2",
    "isbot": "3.6.13",
    "luxon": "3.2.1",
    "mdast-util-gfm-strikethrough": "2.0.0",
    "micromark-extension-gfm-strikethrough": "2.0.0",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-dropzone": "14.2.3",
    "react-markdown": "8.0.3",
    "react-modern-drawer": "1.1.2",
    "react-router": "7.12.0",
    "react-select": "5.7.3",
    "react-syntax-highlighter": "15.6.1",
    "react-tooltip": "5.21.4",
    "remix-client-cache": "^3.0.0",
    "scroll-into-view-if-needed": "3.1.0",
    "textarea-caret": "3.1.0",
    "throttle-debounce": "5.0.0"
  },
  "devDependencies": {
    "@eslint/compat": "1.3.1",
    "@react-router/dev": "7.8.0",
    "@react-router/fs-routes": "7.8.0",
    "@types/cookie": "0.5.1",
    "@types/luxon": "3.0.0",
    "@types/node": "24.0.13",
    "@types/react": "19.1.8",
    "@types/react-dom": "19.1.6",
    "@types/react-syntax-highlighter": "15.5.7",
    "@types/textarea-caret": "3.0.3",
    "@types/throttle-debounce": "5.0.0",
    "@typescript-eslint/eslint-plugin": "8.36.0",
    "@typescript-eslint/parser": "8.36.0",
    "eslint": "9.31.0",
    "eslint-config-prettier": "10.1.5",
    "eslint-import-resolver-typescript": "4.4.4",
    "eslint-plugin-import": "2.32.0",
    "eslint-plugin-jsx-a11y": "6.10.2",
    "eslint-plugin-prettier": "5.5.1",
    "eslint-plugin-react": "7.37.5",
    "eslint-plugin-react-hooks": "5.2.0",
    "globals": "16.3.0",
    "husky": "8.0.1",
    "lint-staged": "16.1.2",
    "prettier": "3.6.2",
    "typescript": "5.8.3",
    "typescript-eslint": "8.36.0",
    "vite": "7.0.8",
    "vite-tsconfig-paths": "5.1.4"
  }
}
